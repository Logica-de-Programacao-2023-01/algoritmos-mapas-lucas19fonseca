package main

import "fmt"

func main() {
	map1 := map[string]int{
		"ursos":    2,
		"pinguins": 4,
	}
	map2 := map[string]int{
		"pinguins": 3,
	}

	fmt.Println(Map(map1, map2))
}

func Map(map1 map[string]int, map2 map[string]int) map[string]int {

	for x, y := range map2 {
		map1[x] = y
	}

	return map1
}
